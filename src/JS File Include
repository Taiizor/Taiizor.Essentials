var taiizor = document.createElement("script");
taiizor.setAttribute("type", "text/javascript");

taiizor.appendChild(document.createTextNode("alert('Testing');"));
document.body.appendChild(taiizor);



document.body.innerHTML += '<script type="text/javascript">function Test() { alert("Testing") }</script>';
document.body.innerHTML += '<script type="text/javascript">alert("Testing");</script>';



window.document.body.insertAdjacentHTML( 'afterbegin', '<script type="text/javascript">function Test() { alert("Testing") }</script>' );



document.body.appendChild(document.createElement("script").setAttribute("type", "text/javascript"));




document.body.appendChild(new DOMParser().parseFromString('<script type="text/javascript">alert("Testing");</script>', 'text/html').head.childNodes[0]);
document.body.appendChild(new DOMParser().parseFromString('<script type="text/javascript">function Test() { alert("Testing") }</script>', 'text/html').head.childNodes[0]);



document.body.appendChild( document.createRange().createContextualFragment('<script type="text/javascript">alert("Testing");</script>') );



const code = "alert('Testing'); let x = 100";  
const F = new Function(code);  
F();